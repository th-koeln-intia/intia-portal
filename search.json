---
layout: null
---
[
  {% for page in site.pages %}
    {
      {% if page.title != nil %}
        "title"    : "{{ page.title | escape }}",
        "category" : "{{ page.category }}",
        "tags"     : "{{ page.tags | join: ', ' }}",
        "url"      : "{{ page.baseurl }}{{ page.url }}",
        "date"     : "{{ page.date }}"
      {% endif %}
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]